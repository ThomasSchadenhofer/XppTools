				<Name>calcInProgress</Name>
				<Source><![CDATA[
    void calcInProgress()
    {
        ;
        if (this.ExportStatus == DEVIntegExportStatus::Sent &&
            this.SendMessageID == this.ConfirmedMessageID)
        {
            this.InProgress = false;
        }
        else
        {
            this.InProgress = true;
        }
    
    }

