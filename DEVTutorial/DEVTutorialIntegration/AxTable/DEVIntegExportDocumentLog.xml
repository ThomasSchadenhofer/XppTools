			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static DEVIntegExportDocumentLog find(CompanyId   _companyId,
                                          VendAccount _vendAccount,
                                          DEVIntegMessageTypeOutbound      _messageTypeOutbound,
                                          boolean     _forUpdate = false)
    {
        DEVIntegExportDocumentLog DEVIntegExportDocumentLog;
    
        if (_companyId && _vendAccount)
        {
            DEVIntegExportDocumentLog.selectForUpdate(_forUpdate);
    
            select firstonly DEVIntegExportDocumentLog
                where DEVIntegExportDocumentLog.CompanyId   == _companyId
                   && DEVIntegExportDocumentLog.VendAccount == _vendAccount
                   && DEVIntegExportDocumentLog.MessageTypeOutbound == _messageTypeOutbound;
        }
    
        return DEVIntegExportDocumentLog;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findFromCommon</Name>
				<Source><![CDATA[
    static DEVIntegExportDocumentLog findFromCommon(Common _common, DEVIntegMessageTypeOutbound _messageTypeOutbound, boolean _forupdate = false)
    {
        DEVIntegExportDocumentLog integExportDocumentLog;
        ;
        if (_common.RecId)
        {
            integExportDocumentLog.selectForUpdate(_forupdate);
    
            select integExportDocumentLog
                where integExportDocumentLog.RefTableId == _common.TableId
                    && integExportDocumentLog.RefRecId == _common.RecId
                    && integExportDocumentLog.MessageTypeOutbound == _messageTypeOutbound;
        }
        return integExportDocumentLog;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    static DEVIntegExportDocumentLog findRecId(RecId _recId, boolean _forupdate = false)
    {
        DEVIntegExportDocumentLog integExportDocumentLog;
        ;
        if ( _recId)
        {
            integExportDocumentLog.selectForUpdate(_forupdate);
            select integExportDocumentLog where
                integExportDocumentLog.RecId == _recId;
        }
        return integExportDocumentLog;
    }

]]></Source>
			</Method>
			<Method>
				<Name>sentRecordExists</Name>
				<Source><![CDATA[
    public static boolean sentRecordExists(Common _common, DEVIntegMessageTypeOutbound _messageOutboundType)
    {
        DEVIntegExportDocumentLog integExportDocumentLog;
    
        select RecId from integExportDocumentLog
            where integExportDocumentLog.RefTableId == _common.TableId
                && integExportDocumentLog.RefRecId == _common.RecId
                && integExportDocumentLog.MessageTypeOutbound == _messageOutboundType
                && integExportDocumentLog.ExportStatus   == DEVIntegExportStatus::ToSend;
    
        if (integExportDocumentLog.RecId)
        {
            return true;
        }
    
        return false;
    }

]]></Source>
