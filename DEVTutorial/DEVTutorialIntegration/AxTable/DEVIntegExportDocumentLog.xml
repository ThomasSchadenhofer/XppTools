			<Method>
				<Name>initFromCommon</Name>
				<Source><![CDATA[
    public void initFromCommon(Common _common, DEVIntegMessageTypeOutbound _messageTypeOutbound)
    {
        VendInvoiceJour         vendInvoiceJour;
        VendTable               vendTable;
    
        this.RefTableId     =   _common.TableId;
        this.RefRecId       =   _common.RecId;
        this.CompanyId      =   _common.dataAreaId;
        this.MessageTypeOutbound    =   _messageTypeOutbound;
    
        if (_common.TableId == tableNum(VendInvoiceJour))
        {
            vendInvoiceJour = _common;
            this.VendAccount = vendInvoiceJour.InvoiceAccount;
        }
        else if (_common.TableId == tableNum(VendTable))
        {
            vendTable = _common;
            this.VendAccount = vendTable.AccountNum;
        }
    }

]]></Source>
